spawn(function()
    while wait() do
        pcall(function()
            if _G.AutoFarmBounty then
                for i,v in pairs(game:GetService("Players").LocalPlayer.Character:GetChildren()) do
                    if v:IsA("Shirt") then
                        v:Destroy()
                    end
                    if v:IsA("Pants") then
                        v:Destroy()
                    end
                    if v:IsA("Accessory") then
                        v:Destroy()
                    end
                end
            end
        end)
    end
end)

