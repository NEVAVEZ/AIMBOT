			tween:Create(Nof2,tweeninfo(0.2),{Size = UDim2.new(0, 0, 0, 65)}):Play()
			wait(0.2)
			Nof2.Visible = false
			game.Debris:AddItem(Nof1,0)
		end)
	end
   
    
    function Update:Notification(textdesc)
        local NotificationHold = Instance.new("TextButton")
